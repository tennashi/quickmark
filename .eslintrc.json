{
  "env": {
    "browser": true,
    "es6": true,
    "webextensions": true
  },
  "extends": [
    "prettier",
    "prettier/@typescript-eslint",
    "prettier/standard"
  ],
  "plugins": [
    "@typescript-eslint",
    "prettier",
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "rules": {
  }
}
